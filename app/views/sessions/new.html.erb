<h1>Adnat</h1>
<h1>Log in</h1>
<%= form_with model: @user, url: sessions_path do |f| %>
  <div>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <p>
    <%= f.submit 'Login' %>
  </p>

  <p> 
    <%= link_to ' Sign Up', new_user_path %><br>
  </p>
  
<% end %>